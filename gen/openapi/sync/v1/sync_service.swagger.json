          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1UpdateGroupRequest"
            }
          }
        ],
        "tags": [
          "SyncService"
        ]
      }
    }
  },
